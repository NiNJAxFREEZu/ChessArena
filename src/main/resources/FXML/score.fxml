<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="scoreboard" prefHeight="720.0" prefWidth="1280.0"
      xmlns="http://javafx.com/javafx/11.0.1" fx:controller="sample.GUI.ScoreboardController">
   <children>
      <AnchorPane prefHeight="720.0" prefWidth="1280.0">
         <children>
            <Accordion layoutX="181.0" layoutY="44.0"/>
            <TableView fx:id="playersTable" layoutX="-2.0" layoutY="64.0" prefHeight="654.0" prefWidth="1280.0">
               <columns>
                  <TableColumn fx:id="nameCol" editable="false" prefWidth="96.0" text="Name"/>
                  <TableColumn fx:id="surnameCol" editable="false" minWidth="7.0" prefWidth="121.0" text="Surname"/>
                  <TableColumn fx:id="ratingCol" editable="false" minWidth="4.0" prefWidth="131.0" text="Rating"/>
                  <TableColumn fx:id="titleCol" editable="false" minWidth="1.0" prefWidth="120.0" text="Title"/>
                  <TableColumn fx:id="genderCol" editable="false" prefWidth="110.0" text="Gender"/>
                  <TableColumn fx:id="clubCol" editable="false" prefWidth="120.0" text="Club"/>
                  <TableColumn fx:id="licenseCol" editable="false" prefWidth="120.0" text="Licence no"/>
                  <TableColumn fx:id="scoreCol" editable="false" prefWidth="120.0" text="Score"/>
               </columns>
            </TableView>
            <Label alignment="CENTER" layoutX="237.0" layoutY="14.0" text="Scoreboard" AnchorPane.leftAnchor="579.0"
                   AnchorPane.rightAnchor="579.0">
               <font>
                  <Font size="24.0"/>
               </font>
            </Label>
            <Button fx:id="closeScoreBtn" layoutX="14.0" layoutY="19.0" mnemonicParsing="false"
                    onAction="#closeScoreboard" text="Close"/>
         </children>
      </AnchorPane>
   </children>
</VBox>
